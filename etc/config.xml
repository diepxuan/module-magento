<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <web>
            <default>
                <cms_no_route>home</cms_no_route>
            </default>
            <seo>
                <use_rewrites>1</use_rewrites>
            </seo>
            <secure>
                <use_in_frontend>1</use_in_frontend>
                <use_in_adminhtml>1</use_in_adminhtml>
            </secure>
        </web>
        <catalog>
            <seo>
                <search_terms>0</search_terms>
                <product_use_categories>1</product_use_categories>
                <generate_category_product_rewrites>1</generate_category_product_rewrites>
            </seo>
        </catalog>
        <admin>
            <security>
                <use_form_key>0</use_form_key>
            </security>
        </admin>
        <currency>
            <options>
                <allow>USD,EUR,VND</allow>
                <base>VND</base>
                <default>VND</default>
            </options>
        </currency>
        <general>
            <country>
                <default>VN</default>
            </country>
            <locale>
                <timezone>Asia/Ho_Chi_Minh</timezone>
                <code>vi_VN</code>
                <weight_unit>kgs</weight_unit>
                <firstday>1</firstday>
                <weekend>0,6</weekend>
            </locale>
        </general>
    </default>
    <stores>
        <admin>
        </admin>
    </stores>
    <websites>
        <admin>
        </admin>
    </websites>
</config>
